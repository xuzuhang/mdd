.mySuppyfooter{
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 1000;
	width: 100%;
	background: #fff;
	height: 160rpx;
	color: #fff;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	border-top:1rpx solid #f3f3f3;
}
.mySuppyfooter .btns{
