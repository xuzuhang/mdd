iew><view class="mianItem2Mid overs">{{''+item.address+''}}</view><block wx:if="{{item.loc}}"><view class="mianItem2Right overs"><label class="iconfont icon-weizhi1 _span" style="font-size:25rpx;"></label>{{''+item.loc+''}}</view></block></view></view></block><view class="pad"></view></view><view class="footer"><view data-event-opts="{{[['tap',[['lowers',['$event']]]]]}}" style="{{'background:'+(obj.online?' linear-gradient(to right,#17c4b0,#1ac8b1,#28d2b5,#36e0b9,#3fe9bc)':'#999')+';'}}" bindtap="__e">结束报价</view></view></view>