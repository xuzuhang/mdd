.pad {
	padding-top: var(--status-bar-height);
	height: 190rpx;
	width: 100%;
}
.headers {
	position: fixed;
	width: 100%;
	z-index: 1000;
	top: 0;
	left: 0;
}
.header {
	height: 90rpx;
	width: 100%;
	padding-top: var(--status-bar-height);
	background: #f8f8f8;
	line-height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	text-align: center;
}
.header .back {
	height: 100%;
	width: 75rpx;
	color: #888888;
	font-size: 35rpx;
}
.header .search {
	width: 562rpx;
	height: 54rpx;
	border: 1rpx solid #ebebeb;
	color: #9f9f9f;
	background: #fff;
	border-radius: 28rpx;
	line-height: 54rpx;
	margin-top: 16rpx;
	font-size: 25rpx;
}
.header .search ._span {
	font-size: 25rpx;
	margin-right: 10rpx;
}
.header .share {
	width: 75rpx;
	color: #333;
	font-size: 35rpx;
}
.screen {
	height: 99rpx;
	line-height: 99rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	border-bottom: 1rpx solid #f1f1f1;
	width: 100%;
	background: #fff;
}
.screen-item {
	width: 25%;
	text-align: center;
	font-size: 25rpx;
	color: #333;
	overflow: hidden;
}
.screen-item .triangle {
	display: inline-block;
	border-top: #000000 solid 7rpx;
	border-left: transparent solid 7rpx;
	border-right: transparent solid 7rpx;
	border-bottom: transparent solid 7rpx;
	margin: 46rpx 0 0 7rpx;
}
.scrolls {}
.scrollItem {
	height: 260rpx;
	width: 100%;
