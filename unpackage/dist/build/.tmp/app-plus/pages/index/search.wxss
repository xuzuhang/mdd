
.pad{
	height: 48rpx;
	width: 100%;
	background: #fff;
	position: fixed;
	top:0;
	z-index: 1001;
}
.headerout{
	position: fixed;
	top:48rpx;
	width: 100%;
	height: 82rpx;
	background: #fff;
	z-index: 1001;
}
.header{
	width: 682rpx;
	height: 82rpx;
	border: 1rpx solid #e0e0e0;
	border-radius:25rpx;
	line-height: 82rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	
	margin: 0 auto;
}
.header .icon-sousuo{
	font-size:24rpx;
	width: 56rpx;
	text-align: center;
}
.header .icon-shanchu{
	font-size:24rpx;
	text-align: center;
	color: #cccccc;
}
.header input{
	display: inline-block;
	width: 569rpx;
	height: 50rpx;
	margin-top: 16rpx;
	font-size: 28rpx;
}
.searchResult{
	position: absolute;
	top: 133rpx;
	width: 100%;
	background:#fff;
	left: 0;
	z-index: 1000;
	min-height:80vh;
}
.searchResultItem{
	height: 100rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding-left: 35rpx;
	line-height: 100rpx;
	border-bottom:1rpx solid #f7f7f7;
	font-size: 28rpx;
}
.center {
	position: fixed;
	top: 180rpx;
	width: 100%;
}
.center .title{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	line-height: 57rpx;
	font-size: 32rpx;
	margin:  0 0 26rpx;
}
.center image{
	margin: 0 22rpx 0 40rpx;
	width: 57rpx;
	height: 57rpx;
}
.commonLists{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	text-align: center;
	border-top: 1rpx solid #f6f6f6;
}
.commonLists .commonListsItem{
	font-size: 22rpx;
	width: 25%;
	height: 94rpx;
	line-height: 94rpx;
	border-bottom: 1rpx solid #f6f6f6;
	position: relative;
}
.commonLists .commonListsItem:after{
	position: absolute;
	content: '';
	display: block;
	width: 1rpx;
	height: 58rpx;
	background: #CCCCCC;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	right: 0;
}


