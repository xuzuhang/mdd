
page{
	background: #f6f6f6;
}
.header{
	width: 100%;
	height: 197rpx;
	background: #fff;
	margin:2rpx 0 22rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	overflow: hidden;
}
.header>view{
	margin: 0 auto;
	width: 675rpx;
}
.header .headerItem1{
	margin: 30rpx auto 10rpx;
	font-weight: bold;
	font-size: 27rpx;
	color: #333;
}
.headerItem2{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 25rpx;
	line-height: 53rpx;
}
.headerItem2Left{
	width: 360rpx;
}
.headerItem3{
	font-size: 25rpx;
	line-height: 53rpx;
}
.mianItem{
	height: 180rpx;
	border-bottom:#f6f6f6 solid 1rpx;
	background: #fff;
	width: 100%;
	line-height: 58rpx;
	overflow: hidden;
}
.mianItem>view{
	height: 58rpx;
	width: 680rpx;
	margin: 0 auto;
}
.mianItem .mianItem1{
	margin-top: 38rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.mianItem .mianItem1 .prices{
	font-size: 32rpx;
	color: #14c5b1;
	width: 197rpx;
	font-weight: bold;
}
.midname{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	width: 298rpx;
}
.midname .names{
	max-width: 149rpx;
	font-size: 30rpx;
}
.icon-grounp{
	   margin-left: 26rpx;
	   display: -webkit-box;
	   display: -webkit-flex;
	   display: -ms-flexbox;
	   display: flex;
	   margin-top: 16rpx;
}
.icon-grounp>view{
		 text-align: center;
		 width: 26rpx;
		 height: 26rpx;
		 color: #fff;
		 font-size: 20rpx;
		 border-radius:5rpx;
		 line-height: 26rpx;
		 margin-right: 9rpx;
		margin-top: 4rpx;
}
.lookbigImg{
	width: 100rpx;
	height: 41rpx;
	margin: 8rpx;
	
	position: relative;
}
.lookbigImg .innerImg{
	position: absolute;
	top: -40rpx;
	left: 0;
	height: 150rpx;
	z-index: 1000;
	width: 200rpx;
	background: transparent;
}
.lookbigImg .innerImg view{
	width: 100rpx;
	height: 41rpx;
	background: #24bf9f;
	color:#fff;
	font-size: 24rpx;
	border-radius:5rpx;
	text-align: center;
	line-height: 41rpx;
	margin-left: 55rpx;
	position: relative;
	margin-top: 35rpx;
}
.mianItem2{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.mianItem2Left{
	width: 197rpx;
	font-size: 33rpx;
}
.mianItem2Mid{
	width: 298rpx;
	font-size: 33rpx;
}
.mianItem2Right{
	font-size: 24rpx;
	color: #999;
	width: 163rpx;
	text-align: right;
}
.footer{
	position: fixed;
	bottom: 0;
	height: 160rpx;
	z-index: 100;
	background: #fff;
	width: 100%;
}
.pad{
	height: 160rpx;
}
.footer view{
	text-align: center;
	width: 710rpx;
	height: 100rpx;
	margin: 30rpx auto;
	color: #fff;
	line-height: 100rpx;
	border-radius:10rpx;
	font-size: 38rpx;
	background: -webkit-gradient(linear,left top, right top,from(#17c4b0),color-stop(#1ac8b1),color-stop(#28d2b5),color-stop(#36e0b9),to(#3fe9bc));
	background: -o-linear-gradient(left,#17c4b0,#1ac8b1,#28d2b5,#36e0b9,#3fe9bc);
	background: linear-gradient(to right,#17c4b0,#1ac8b1,#28d2b5,#36e0b9,#3fe9bc);
}

