	background: -o-linear-gradient(left,#17c5af,#23d0b2,#3ce6bb);
	background: linear-gradient(to right,#17c5af,#23d0b2,#3ce6bb);
	overflow: hidden;
}
.mySuppyfooter .btns ._span{
