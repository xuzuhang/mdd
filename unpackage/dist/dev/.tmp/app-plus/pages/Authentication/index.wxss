
page {
	background: #f6f6f6;
}
.header {
	margin-top: 20rpx;
}
.header-item {
	background: #fff;
	margin-bottom: 2rpx;
	height: 140rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.header-item-left {
	height: 100%;
	width: 462rpx;
	overflow: hidden;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.header-item-left .title {
	font-size: 30rpx;
	color: #cccccc;
	margin-left: 32rpx;
}
.header-item-left .title view:nth-of-type(1) {
	font-weight: bold;
	color: #000;
	font-size: 35rpx;
	margin-top: 28rpx;
}
.header-item-right {
	width: 111rpx;
	height: 54rpx;
	border-radius: 27rpx;
	border: 1rpx solid #16c4af;
	color: #16c4af;
	line-height: 54rpx;
	text-align: center;
	font-size: 23rpx;
	margin-right: 20rpx;
	margin-top: 43rpx;
}
.header-item-right.active {
	background: #16c4af;
	color: #fff;
}
.header-item-left .icon {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	width: 98rpx;
	height: 98rpx;
	border-radius: 50%;
	margin: 21rpx 0 0 18rpx;
}
.footer {
	width: 711rpx;
	height: 105rpx;
	line-height: 105rpx;
	background: -webkit-gradient(linear, left top, right top, from(#17c5af), color-stop(#20cdb2), color-stop(#37e0b9), to(#3fe9bb));
	background: -o-linear-gradient(left, #17c5af, #20cdb2, #37e0b9, #3fe9bb);
	background: linear-gradient(to right, #17c5af, #20cdb2, #37e0b9, #3fe9bb);
	color: #fff;
	font-size: 44rpx;
	position: absolute;
	text-align: center;
	border-radius: 10rpx;
	bottom: 40rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	    -ms-transform: translateX(-50%);
	        transform: translateX(-50%);
}
.footer ._span {
	font-size: 50rpx;
	margin-right: 10rpx;
}

