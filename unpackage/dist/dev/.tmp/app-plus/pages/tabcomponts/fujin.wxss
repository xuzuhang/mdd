.header{
		z-index: 300;
		background: #fff;
		left: 0;
		top: 0;
		padding-top: var(--status-bar-height);
		height: 97rpx;
		line-height: 97rpx;
		text-align: center;
		width: 100%;
		position: fixed;
		display: -webkit-box;
